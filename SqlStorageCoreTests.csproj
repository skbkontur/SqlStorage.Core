      <TargetFramework>net472</TargetFramework>
      <RootNamespace>SKBKontur.EDIFunctionalTests.SqlStorageCoreTests</RootNamespace>
      <AssemblyName>Catalogue.EDIFunctionalTests.SqlStorageCoreTests</AssemblyName>
      <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
